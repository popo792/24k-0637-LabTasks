#include <iostream>
using namespace std;

void bubbleSort(int arr[], int n, int &c) {
    c = 0;
    for (int i = 0; i < n - 1; i++)
        for (int j = 0; j < n - i - 1; j++) {
            c++;
            if (arr[j] > arr[j + 1]) {
                int t = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = t;
            }
        }
}

int main() {
    int arr[] = {64, 34, 25, 12, 22, 11, 90};
    int n = sizeof(arr) / sizeof(arr[0]), c;
    bubbleSort(arr, n, c);
    for (int i = 0; i < n; i++) cout << arr[i] << " ";
    cout << endl << c << endl;
}
